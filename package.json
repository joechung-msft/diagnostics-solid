    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.17.2",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.33.0",
    "eslint-plugin-solid": "^0.14.5",
    "jiti": "^2.5.1",
    "jsdom": "^26.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1",
    "vite": "^6.3.5",
    "vite-plugin-solid": "*",
    "vitest": "^3.2.4"
